{
  "workbench.colorTheme": "Default Light Modern",
  "workbench.iconTheme": "my-icons",
  "window.nativeTabs": true,
  "zenMode.fullScreen": false,
  "go.toolsManagement.autoUpdate": true,
  // "editor.fontFamily": "'Hack', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontFamily": "'JetBrainsMono Nerd Font Mono', Menlo, Monaco, 'Courier New', monospace",
  // "workbench.fontAliasing": "antialiased",
  "editor.renderWhitespace": "selection",
  "workbench.editor.showTabs": "single",
  "zenMode.showTabs": "single",
  "window.title": "${dirty}${remoteName}${separator}${rootPath}${separator}${activeEditorShort}${separator}${profileName}",
  "window.titleSeparator": " ... ",
  "window.density.editorTabHeight": "compact",
  "files.autoSave": "afterDelay",
  "editor.hideCursorInOverviewRuler": true,
  "editor.scrollbar.horizontalScrollbarSize": 6,
  "editor.scrollbar.verticalScrollbarSize": 6,
  "editor.scrollbar.horizontal": "hidden",
  "editor.hover.delay": 1000,
  "workbench.tree.indent": 16,
  "window.openFoldersInNewWindow": "off",
  "editor.semanticHighlighting.enabled": "configuredByTheme",
  "workbench.editor.centeredLayoutFixedWidth": true,
  "zenMode.silentNotifications": false,
  "zenMode.hideLineNumbers": false,
  "telemetry.telemetryLevel": "error",
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "window.titleBarStyle": "native",
  "git.autofetch": true,
  "workbench.editor.labelFormat": "medium",
  "git.confirmSync": false,
  "terminal.integrated.fontFamily": "Hurmit Nerd Font Mono",
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "gopls": {
    "ui.semanticTokens": true
  },
  "symbols.hidesExplorerArrows": false,
  "editor.accessibilitySupport": "off",
  "bookmarks.gutterIconFillColor": "#fa9e00",
  "bookmarks.gutterIconBorderColor": "#fa9e00",
  "bookmarks.sideBar.expanded": true,
  "files.exclude": {
    "**/.DS_Store": true,
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.unicodeHighlight.nonBasicASCII": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.experimental.showMoves": true,
  "workbench.editor.enablePreview": false,
  "redhat.telemetry.enabled": false,
  "editor.guides.indentation": false,
  "workbench.colorCustomizations": {
    "sideBar.background": "#fff",
    "sideBar.border": "#adb4bd",
    "activityBar.border": "#bdc4d1",
    "panel.border": "#bdc4d1",
    "editor.lineHighlightBackground": "#fffae3",
    "editorBracketHighlight.foreground1": "#000000",
    "editorBracketHighlight.foreground2": "#000000",
    "editorBracketHighlight.foreground3": "#000000",
    "editorBracketHighlight.foreground4": "#000000",
    "editorBracketHighlight.unexpectedBracket.foreground": "#000000",
  },
  "editor.tokenColorCustomizations": {
    "[Default Light Modern]": {
      "strings": "#07961D",
      "numbers": "#1750eb",
      "functions": "#000",
      "comments": "#9E9E9E",
      "textMateRules": [
        {
          "name": "Function declarations",
          "scope": [
            "entity.name.function",
            "support.function",
            "support.constant.handlebars",
            "source.powershell variable.other.member",
            "entity.name.operator.custom-literal" // See https://en.cppreference.com/w/cpp/language/user_literal
          ],
          "settings": {
            "foreground": "#000"
          }
        },
        {
          "name": "Types declaration and references",
          "scope": [
            "support.class",
            "support.type",
            "entity.name.type",
            "entity.other.attribute",
            "entity.name.scope-resolution",
            "entity.name.class",
            "storage.type.numeric.go",
            "storage.type.byte.go",
            "storage.type.boolean.go",
            "storage.type.string.go",
            "storage.type.uintptr.go",
            "storage.type.error.go",
            "storage.type.rune.go",
            "storage.type.cs",
            "storage.type.generic.cs",
            "storage.type.modifier.cs",
            "storage.type.variable.cs",
            "storage.type.annotation.java",
            "storage.type.generic.java",
            "storage.type.java",
            "storage.type.object.array.java",
            "storage.type.primitive.array.java",
            "storage.type.primitive.java",
            "storage.type.token.java",
            "storage.type.groovy",
            "storage.type.annotation.groovy",
            "storage.type.parameters.groovy",
            "storage.type.generic.groovy",
            "storage.type.object.array.groovy",
            "storage.type.primitive.array.groovy",
            "storage.type.primitive.groovy"
          ],
          "settings": {
            "foreground": "#000"
          }
        },
        {
          "name": "Control flow / Special keywords",
          "scope": [
            "keyword.control",
            "source.cpp keyword.operator.new",
            "source.cpp keyword.operator.delete",
            "keyword.other.using",
            "keyword.other.directive.using",
            "keyword.other.operator",
            "entity.name.operator"
          ],
          "settings": {
            "foreground": "#003ACC"
          }
        },
        {
          "name": "Variable and parameter name",
          "scope": [
            "variable",
            "meta.definition.variable.name",
            "support.variable",
            "entity.name.variable",
            "constant.other.placeholder", // placeholders in strings
          ],
          "settings": {
            "foreground": "#000"
          }
        },
        {
          "name": "Constants and enums",
          "scope": [
            "variable.other.constant",
            "variable.other.enummember"
          ],
          "settings": {
            "foreground": "#9C13AB",
          }
        },
        {
          "name": "Language Constants",
          "scope": [
            "constant.language",
            "support.constant",
            "variable.language"
          ],
          "settings": {
            "foreground": "#003ACC"
          }
        },
        {
          "name": "Import Package Name",
          "scope": [
            "entity.name.namespace",
          ],
          "settings": {
            "foreground": "#8C6200"
          }
        },
        {
          "scope": "constant.character.escape",
          "settings": {
            "foreground": "#003ACC"
          }
        },
      ]
    }
  },
  "todohighlight.keywords": [
    {
      "text": "TODO",
      "color": "#0099F2",
      "backgroundColor": "#fff",
      "isWholeLine": true,
    },
    {
      "text": "FIXME",
      "color": "#0099F2",
      "backgroundColor": "#fff",
      "isWholeLine": true,
    },
    {
      "text": "go:generate",
      "color": "#000",
      "backgroundColor": "#fff",
      "isWholeLine": true,
    },
    {
      "text": "\\n",
      "color": "#003ACC",
      "backgroundColor": "#fff"
    },
  ],
  "peacock.showColorInStatusBar": false,
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#dd0531"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#61dafb"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Svelte Orange",
      "value": "#ff3d00"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    },
    {
      "name": "Color of Zhopa",
      "value": "#f9ca9c"
    },
    {
      "name": "Grey",
      "value": "#b4baa9"
    },
    {
      "name": "Siniy",
      "value": "#9badd5"
    },
    {
      "name": "Man",
      "value": "#fef8cd"
    }
  ],
  "[proto3]": {
    "editor.defaultFormatter": "zxh404.vscode-proto3"
  },
  "codeium.enableConfig": {
    "*": true
  },
  "workbench.statusBar.visible": false,
}