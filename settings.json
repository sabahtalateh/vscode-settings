{
  "workbench.colorTheme": "Extra Light",
  "workbench.iconTheme": "my-icons",
  "zenMode.fullScreen": false,
  "go.toolsManagement.autoUpdate": true,
  "editor.fontFamily": "'Hack', Menlo, Monaco, 'Courier New', monospace",
  // "editor.fontFamily": "'JetBrainsMono Nerd Font Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 12.5,
  "window.title": "${dirty}${remoteName}${separator}${rootPath}${separator}${activeEditorShort}${separator}${profileName}",
  "window.titleSeparator": " ðŸ“¦ ",
  "window.density.editorTabHeight": "compact",
  "files.autoSave": "afterDelay",
  "editor.hideCursorInOverviewRuler": true,
  "editor.scrollbar.horizontalScrollbarSize": 6,
  "editor.scrollbar.verticalScrollbarSize": 6,
  "editor.scrollbar.horizontal": "hidden",
  "editor.hover.delay": 1000,
  "workbench.tree.indent": 12,
  "window.openFoldersInNewWindow": "off",
  "editor.semanticHighlighting.enabled": "configuredByTheme",
  "workbench.editor.centeredLayoutFixedWidth": true,
  "telemetry.telemetryLevel": "error",
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "git.autofetch": true,
  "workbench.editor.labelFormat": "medium",
  "git.confirmSync": false,
  "terminal.integrated.fontFamily": "Hurmit Nerd Font Mono",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "gopls": {
    "ui.semanticTokens": true
  },
  "editor.accessibilitySupport": "off",
  "files.exclude": {
    "**/.DS_Store": true,
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.unicodeHighlight.nonBasicASCII": true,
  "diffEditor.experimental.showMoves": true,
  "workbench.editor.enablePreview": false,
  "redhat.telemetry.enabled": false,
  "editor.guides.indentation": false,
  "[proto3]": {
    "editor.defaultFormatter": "zxh404.vscode-proto3"
  },
  "explorer.autoReveal": false,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[sql]": {
    "editor.defaultFormatter": "cweijan.vscode-postgresql-client2"
  },
  "sqltools.format": {},
  "sqltools.highlightQuery": false,
  "workbench.tree.expandMode": "doubleClick",
  "diffEditor.ignoreTrimWhitespace": false,
  "cmake.configureOnOpen": true,
  "cmake.showOptionsMovedNotification": false,
  "projectManager.sortList": "Recent",
  "workbench.list.horizontalScrolling": true,
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.scrollback": 5000,
  "chat.commandCenter.enabled": false,
  "workbench.editor.showTabs": "single",
  "window.customTitleBarVisibility": "never",
}
