{
  "workbench.colorTheme": "Extra Light",
  "workbench.iconTheme": "my-icons",
  "window.nativeTabs": true,
  "zenMode.fullScreen": false,
  "go.toolsManagement.autoUpdate": true,
  // "editor.fontFamily": "'Hack', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontFamily": "'JetBrainsMono Nerd Font Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 12.5,
  // "workbench.fontAliasing": "antialiased",
  "editor.renderWhitespace": "selection",
  "workbench.editor.showTabs": "single",
  "zenMode.showTabs": "single",
  "window.title": "${dirty}${remoteName}${separator}${rootPath}${separator}${activeEditorShort}${separator}${profileName}",
  "window.titleSeparator": " ... ",
  "window.density.editorTabHeight": "compact",
  "files.autoSave": "afterDelay",
  "editor.hideCursorInOverviewRuler": true,
  "editor.scrollbar.horizontalScrollbarSize": 6,
  "editor.scrollbar.verticalScrollbarSize": 6,
  "editor.scrollbar.horizontal": "hidden",
  "editor.hover.delay": 1000,
  "workbench.tree.indent": 16,
  "window.openFoldersInNewWindow": "off",
  "editor.semanticHighlighting.enabled": "configuredByTheme",
  "workbench.editor.centeredLayoutFixedWidth": true,
  "zenMode.silentNotifications": false,
  "zenMode.hideLineNumbers": false,
  "telemetry.telemetryLevel": "error",
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "window.titleBarStyle": "native",
  "git.autofetch": true,
  "workbench.editor.labelFormat": "medium",
  "git.confirmSync": false,
  "terminal.integrated.fontFamily": "Hurmit Nerd Font Mono",
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "gopls": {
    "ui.semanticTokens": true
  },
  "editor.accessibilitySupport": "off",
  "bookmarks.gutterIconFillColor": "#fa9e00",
  "bookmarks.gutterIconBorderColor": "#fa9e00",
  "bookmarks.sideBar.expanded": true,
  "files.exclude": {
    "**/.DS_Store": true,
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.unicodeHighlight.nonBasicASCII": true,
  "diffEditor.experimental.showMoves": true,
  "workbench.editor.enablePreview": false,
  "redhat.telemetry.enabled": false,
  "editor.guides.indentation": false,
  "todohighlight.keywords": [
    {
      "text": "TODO",
      "color": "#0099F2",
      "backgroundColor": "#fff",
      "isWholeLine": true,
    },
    {
      "text": "FIXME",
      "color": "#0099F2",
      "backgroundColor": "#fff",
      "isWholeLine": true,
    },
    {
      "text": "go:generate",
      "color": "#000",
      "backgroundColor": "#fff",
      "isWholeLine": true,
    },
    {
      "text": "\\n",
      "color": "#003ACC",
      "backgroundColor": "#fff"
    },
  ],
  "peacock.showColorInStatusBar": false,
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#dd0531"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#61dafb"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Svelte Orange",
      "value": "#ff3d00"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    },
    {
      "name": "Color of Zhopa",
      "value": "#f9ca9c"
    },
    {
      "name": "Grey",
      "value": "#b4baa9"
    },
    {
      "name": "Siniy",
      "value": "#9badd5"
    },
    {
      "name": "Man",
      "value": "#fef8cd"
    }
  ],
  "[proto3]": {
    "editor.defaultFormatter": "zxh404.vscode-proto3"
  },
  "codeium.enableConfig": {
    "*": true
  },
  "explorer.autoReveal": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "symbols.hidesExplorerArrows": false,
  "[sql]": {
    "editor.defaultFormatter": "cweijan.vscode-postgresql-client2"
  },
  "sqltools.format": {},
  "sqltools.highlightQuery": false,
  "workbench.tree.expandMode": "doubleClick",
  "diffEditor.ignoreTrimWhitespace": false,
}
