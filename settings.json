{
  "workbench.colorTheme": "Default Light Modern",
  "workbench.colorCustomizations": {
    "sideBar.background": "#fff",
    "sideBar.border":"#adb4bd",
    "activityBar.border": "#bdc4d1",
    "panel.border": "#bdc4d1",
  },
  "workbench.iconTheme": "my-icons",
  "window.nativeTabs": true,
  "zenMode.fullScreen": false,
  "go.toolsManagement.autoUpdate": true,
  // "editor.fontFamily": "'Hack', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontFamily": "'JetBrainsMono Nerd Font Mono', Menlo, Monaco, 'Courier New', monospace",
  // "editor.fontFamily": "'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  // "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 12,
  // "workbench.fontAliasing": "antialiased",
  "editor.renderWhitespace": "selection",
  "workbench.editor.showTabs": "single",
  "zenMode.showTabs": "single",
  "window.title": "${dirty}${remoteName}${separator}${rootPath}${separator}${activeEditorShort}${separator}${profileName}",
  "window.titleSeparator": " ... ",
  "window.density.editorTabHeight": "compact",
  "files.autoSave": "afterDelay",
  "editor.hideCursorInOverviewRuler": true,
  "editor.scrollbar.horizontalScrollbarSize": 6,
  "editor.scrollbar.verticalScrollbarSize": 6,
  "editor.scrollbar.horizontal": "hidden",
  "editor.hover.delay": 1000,
  "workbench.tree.indent": 16,
  "window.openFoldersInNewWindow": "off",
  "editor.semanticHighlighting.enabled": "configuredByTheme",
  "workbench.editor.centeredLayoutFixedWidth": true,
  "zenMode.silentNotifications": false,
  "zenMode.hideLineNumbers": false,
  "gitlens.mode.active": "zen",
  "telemetry.telemetryLevel": "error",
  "gitlens.telemetry.enabled": false,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "window.titleBarStyle": "native",
  "git.autofetch": true,
  "workbench.editor.labelFormat": "medium",
  "git.confirmSync": false,
  "terminal.integrated.fontFamily": "Hurmit Nerd Font Mono",
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "gopls": {
    "ui.semanticTokens": true
  },
  "resmon.show.cpuusage": false,
  "resmon.show.mem": false,
  "resmon.disk.drives": null,
  "playwright.reuseBrowser": true,
  "dotnet.unitTests.runSettingsPath": ".runsettings",
  "symbols.hidesExplorerArrows": false,
  "editor.accessibilitySupport": "off",
  "bookmarks.gutterIconFillColor": "#fa9e00",
  "bookmarks.gutterIconBorderColor": "#fa9e00",
  "bookmarks.sideBar.expanded": true,
  "files.exclude": {
    "**/.DS_Store": true,
  },
  "breadcrumbs.enabled": false, // actually true. (bug?)
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.unicodeHighlight.nonBasicASCII": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.experimental.showMoves": true,
  "workbench.editor.enablePreview": false,
  "redhat.telemetry.enabled": false,
  "workbench.statusBar.visible": false,
  "editor.guides.indentation": false,
  "peacock.showColorInStatusBar": false,
